    %script{:src => "javascripts/prism.js"}
    %script{:src => "javascripts/lunr.min.js"}
    %script{:src => "javascripts/search.js"}
    %script{:src => "javascripts/materialize.js"}
    %script{:src => "javascripts/init.js"}
